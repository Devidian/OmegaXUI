<configs>
	<append xpath="/styles">
		<!--OMEGA XUI-->
		<style name="compass.window.indicator" type="sprite">
			<style_entry name="depth" value="10"/>
			<style_entry name="color" value="200,200,0,100"/>
			<style_entry name="pos" value="235,-5"/>
			<style_entry name="width" value="10"/>
			<style_entry name="height" value="33"/>
		</style>
		<style name="oxui.border" type="sprite">
			<style_entry name="name"	value="border"/>
			<style_entry name="depth" value="1"/>
			<style_entry name="color" value="0,0,0,150"/>
			<style_entry name="type" value="sliced"/>
		</style>
		
		<style name="oxui.bg.stats" type="sprite">
			<style_entry name="name"	value="background"/>
			<style_entry name="depth" value="2"/>
			<style_entry name="color" value="64,64,64,100"/>
			<style_entry name="type" value="sliced"/>
			<style_entry name="height" value="28"/>
			<style_entry name="pos" value="2,-2"/>
		</style>
		
		<style name="oxui.fg.stats" type="sprite">
			<style_entry name="name"	value="BarContent"/>
			<style_entry name="depth" value="3"/>
			<style_entry name="type" value="filled"/>
			<style_entry name="height" value="28"/>
			<style_entry name="pos" value="2,-2"/>
			<style_entry name="fill" value="0"/>
		</style>
		
		<!-- Begin Style A -->
		<style name="oxui.style1.bg">
			<style_entry name="depth" value="0"/>
			<style_entry name="texture" value="@http://xui.omega-zirkel.de/style1_background.png"/> <!-- [local|@file:Data\Config\assets\style1_background.png] [www|@http://xui.omega-zirkel.de/style1_background.png]-->
			<style_entry name="type" value="sliced"/>
			<style_entry name="size" value="239,160"/>
			<style_entry name="pos" value="0,160"/>
			<style_entry name="material" value="Materials/Transparent Colored"/>
		</style>
		<style name="oxui.style1.overlay">
			<style_entry name="depth" value="10"/>
			<style_entry name="texture" value="@http://xui.omega-zirkel.de/style1_overlay_01.png"/> <!-- [local|@file:Data\Config\assets\style1_overlay_01.png] [www|@http://xui.omega-zirkel.de/style1_overlay_01.png]-->
			<!--style_entry name="texture" value="@http://xui.omega-zirkel.de/style1_overlay_02.png"/--> <!-- [local|@file:Data\Config\assets\style1_overlay_02.png] [www|@http://xui.omega-zirkel.de/style1_overlay_02.png]-->
			<style_entry name="type" value="sliced"/>
			<style_entry name="size" value="239,160"/>
			<style_entry name="pos" value="0,160"/>
			<style_entry name="material" value="Materials/Transparent Colored"/>
		</style>
		<style name="oxui.style1.icons">
			<style_entry name="depth" value="11"/>
			<style_entry name="texture" value="@http://xui.omega-zirkel.de/style1_icons_01.png"/> <!-- [local|@file:Data\Config\assets\style1_icons_01.png] [www|@http://xui.omega-zirkel.de/style1_icons_01.png]-->
			<style_entry name="type" value="sliced"/>
			<style_entry name="size" value="239,160"/>
			<style_entry name="pos" value="0,160"/>
			<style_entry name="material" value="Materials/Transparent Colored"/>
		</style>
		<style name="oxui.style1.bar" type="sprite">
			<style_entry name="name"	value="BarContent"/>
			<style_entry name="depth" value="1"/>
			<style_entry name="type" value="filled"/>
			<style_entry name="height" value="28"/>
			<style_entry name="width" value="100"/>
			<style_entry name="fill" value="0"/>
		</style>
		<style name="oxui.style1.textA">
			<style_entry name="depth" value="2"/>
			<style_entry name="pos" value="-5,-2"/>
			<style_entry name="height" value="28"/>
			<style_entry name="justify" value="right"/>
			<style_entry name="font_size" value="22"/>
		</style>
		<style name="oxui.style1.textBar">
			<style_entry name="depth" value="2"/>
			<style_entry name="pos" value="0,-2"/>
			<style_entry name="height" value="28"/>
			<style_entry name="justify" value="center"/>
			<style_entry name="font_size" value="24"/>
			<style_entry name="pivot" value="topleft"/>
			<style_entry name="name" value="TextContent"/>
			<style_entry name="color" value="[white]"/>
		</style>
		
		<!-- Begin Style B -->
		<style name="oxui.style2.bg1">
			<style_entry name="depth" value="0"/>
			<style_entry name="texture" value="@http://xui.omega-zirkel.de/style2_background1.png"/> <!-- [local|@file:Data\Config\assets\style1_background.png] [www|@http://xui.omega-zirkel.de/style1_background.png]-->
			<style_entry name="type" value="sliced"/>
			<style_entry name="size" value="340,128"/>
			<style_entry name="pos" value="0,128"/>
			<style_entry name="material" value="Materials/Transparent Colored"/>
		</style>
		<style name="oxui.style2.size.barV" type="rect">
			<style_entry name="height" value="64"/>
			<style_entry name="width" value="24"/>
		</style>
		<style name="oxui.style2.size.statH" type="rect">
			<style_entry name="height" value="24"/>
			<style_entry name="width" value="75"/>
		</style>
		<style name="oxui.style2.bar" type="sprite">
			<style_entry name="name"	value="BarContent"/>
			<style_entry name="depth" value="1"/>
			<style_entry name="type" value="filled"/>
			<style_entry name="filldirection" value="vertical"/>
			<style_entry name="height" value="64"/>
			<style_entry name="width" value="24"/>
			<style_entry name="fill" value="0"/>
		</style>
		<style name="oxui.style2.textA">
			<style_entry name="depth" value="2"/>
			<style_entry name="pos" value="-5,-2"/>
			<style_entry name="height" value="24"/>
			<style_entry name="justify" value="right"/>
			<style_entry name="font_size" value="20"/>
		</style>
		<style name="oxui.style2.radialA" type="sprite">
			<style_entry name="name"	value="BarContent"/>
			<style_entry name="depth" value="1"/>
			<style_entry name="type" value="filled"/>
			<style_entry name="filldirection" value="radial360"/>
			<style_entry name="height" value="128"/>
			<style_entry name="width" value="128"/>
			<style_entry name="fill" value="0"/>
		</style>
		<style name="oxui.style2.radialB" type="sprite">
			<style_entry name="name"	value="BarContent"/>
			<style_entry name="depth" value="1"/>
			<style_entry name="type" value="filled"/>
			<style_entry name="filldirection" value="radial360"/>
			<style_entry name="height" value="76"/>
			<style_entry name="width" value="76"/>
			<style_entry name="fill" value="0"/>
		</style>



		<!-- A Size (28*100) -->
		<style name="oxui.size.a">
			<style_entry name="height" value="28"/>
			<style_entry name="width" value="100"/>
		</style>
		
		<!-- B Size (28*65) -->
		<style name="oxui.size.b">
			<style_entry name="height" value="28"/>
			<style_entry name="width" value="65"/>
		</style>
		
		<!-- hide element -->
		<style name="oxui.invisible">
			<style_entry name="pos" value="0,-0"/>
			<style_entry name="height" value="0"/>
			<style_entry name="width" value="0"/>
			<style_entry name="color" value="0,0,0,0"/>
			<style_entry name="visible" value="false"/>
		</style>
	</append>
</configs>
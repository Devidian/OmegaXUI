<configs>
	<append xpath="/windows">
		<!-- Omega XUI Style 1 START -->
		<window name="OXUIStatFrameStyle1" width="239" height="160">
			<texture style="oxui.style1.bg"/>
			<!--texture style="oxui.style1.overlay"/-->
			<texture style="oxui.style1.icons"/>
			<!-- left side: Stamina, Health, Level, Kills -->
			<rect style="oxui.size.a" pos="32,154" controller="HUDStatBar" stat_type="Stamina" visible="{statvisible}">
				<sprite style="oxui.style1.bar" color="255,237,33,100" />
				<label style="oxui.style1.textBar" text="{statcurrent}%"/>
			</rect>
			<rect style="oxui.size.a" pos="32,114" controller="HUDStatBar" stat_type="Health" visible="{statvisible}">
				<sprite style="oxui.style1.bar" color="196,0,0,100" />
				<label style="oxui.style1.textBar" text="{statcurrent}%"/>
			</rect>
			<rect style="oxui.size.a" pos="32,74" controller="PlayerStatsWindow">
				<sprite style="oxui.style1.bar" color="49,91,203" fill="{playerlevelfill}" />
				<label style="oxui.style1.textBar" text="{playerleveltitle}: [DECEA3]{playerlevel}[-]" />
			</rect>
			<rect style="oxui.size.a" pos="32,34" controller="PlayerStatsWindow">
				<label style="oxui.style1.textA" name="zKillsLabel" text=" [DECEA3]{playerzombiekills}[-] / [DECEA3]{playerpvpkills}[-]"/>
			</rect>
			<!-- right side: water, food temp, elevation -->
			<rect style="oxui.size.b" pos="164,154" controller="HUDStatBar" stat_type="Water" visible="{statvisible}">
				<sprite style="oxui.style1.bar" width="65" color="50,41,224,100" />
				<label style="oxui.style1.textBar" text="{statcurrent}%"/>
			</rect>
			<rect style="oxui.size.b" pos="164,114" controller="HUDStatBar" stat_type="Food" visible="{statvisible}">
				<sprite style="oxui.style1.bar" width="65" color="67,183,32,100" />
				<label style="oxui.style1.textBar" text="{statcurrent}%"/>
			</rect>
			<rect style="oxui.size.b" pos="164,74" controller="PlayerStatsWindow">
				<label style="oxui.style1.textA" name="tempLabel" text=" [DECEA3]{playercoretemp}[-]"/>
			</rect>
			<rect style="oxui.size.b" pos="164,34" controller="MapStats">
				<label style="oxui.style1.textA" name="elevLabel" text=" [DECEA3]{mapelevation} M[-]"/>
			</rect>
			<!-- Buff Frame -->
			<rect name="hud" pos="2,180" side="left" controller="BuffPopoutList" pivot="BottomLeft" >
				<panel width="150" height="32" name="item" visible="false" pivot="right" disableautobackground="true" pos="0, 0" >
					<sprite depth="3" pos="0,0"  name="Background" sprite="ui_game_popup" height="32" width="150" pivot="left" flip="Horizontally" color="[transparent]" />
					<sprite depth="4" name="Icon" size="26,26" pos="8,0" pivot="left" color="[transparent]"/>
					<label depth="6" name="TextContent" pos="0,0" font_size="26" color="[white]" justify="center" pivot="left"/>
				</panel>
			</rect>
			
		</window>
		<!-- Omega XUI Style 1 END -->

		<!-- Omega XUI Style 2 START -->
		<window name="OXUIStatFrameStyle2" width="340" height="128">
			<texture style="oxui.style2.bg1"/>
			
			<rect height="128" width="128" pos="0,128" controller="HUDStatBar" stat_type="Stamina" visible="{statvisible}">
				<sprite style="oxui.style2.radialA" color="255,237,33,100" filldirection="radial360" atlas="ItemIconAtlas" sprite="noneyet" />
			</rect>
			
			<rect height="76" width="76" pos="26,102" controller="HUDStatBar" stat_type="Health" visible="{statvisible}">
				<sprite style="oxui.style2.radialB" color="[black]" type="simple" filldirection="radial360" atlas="ItemIconAtlas" sprite="noneyet" />
				<sprite style="oxui.style2.radialB" depth="2" color="196,0,0,100" filldirection="radial360" atlas="ItemIconAtlas" sprite="noneyet" />
			</rect>
			
			<rect style="oxui.style2.size.barV" pos="137,69" controller="HUDStatBar" stat_type="Food" visible="{statvisible}">
				<sprite style="oxui.style2.bar" color="67,183,32,100" />
			</rect>
			<rect style="oxui.style2.size.barV" pos="169,69" controller="HUDStatBar" stat_type="Water" visible="{statvisible}">
				<sprite style="oxui.style2.bar" color="50,41,224,100" />
			</rect>
			<rect style="oxui.style2.size.barV" pos="201,69" controller="PlayerStatsWindow">
				<sprite style="oxui.style2.bar" color="49,91,203" fill="{playerlevelfill}" />
			</rect>
			
			
			<rect style="oxui.style2.size.statH" pos="257,93" controller="PlayerStatsWindow">
				<label style="oxui.style2.textA" name="zKillsLabel" text=" [DECEA3]{playerzombiekills}[-]"/>
			</rect>
			<rect style="oxui.style2.size.statH" pos="257,61" controller="PlayerStatsWindow">
				<label style="oxui.style2.textA" name="tempLabel" text=" [DECEA3]{playercoretemp}[-]"/>
			</rect>
			<rect style="oxui.style2.size.statH" pos="257,29" controller="MapStats">
				<label style="oxui.style2.textA" name="elevLabel" text=" [DECEA3]{mapelevation} M[-]"/>
			</rect>
			
		</window>
		<!-- Omega XUI Style 2 END -->
	</append>

	<append xpath="/windows/window[@name='windowCompass']">
		<sprite style="compass.window.indicator"/>
	</append>
</configs>